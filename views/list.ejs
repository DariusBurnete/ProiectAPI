<%- include('partials/header.ejs') -%>


<div class="list-container">
    <% if(movies) { %>
        <% movies.forEach(function(movie) { %>
                <div class="list-card">
                    <a href="/movie">
                        <img src="<%= movie.Poster %>">
                        <li><%= movie.Title %></li>
                    </a>
                </div>
        <% }) %>
    <% } %>
</div>


